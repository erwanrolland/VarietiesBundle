{% extends 'LibrinfoCoreBundle:CRUD:base_show.html.twig' %}

{% block navbar_title %}
    {% if object.isStrain == true %}
        {{ 'Strain'|trans() }}: {{ admin.toString(object)|truncate(15) }}
    {% else %}
        {{ admin.label|trans() }}: {{ admin.toString(object)|truncate(15) }}
    {% endif %}
{% endblock %}

{% block show %}
    {{ parent() }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('bundles/librinfovarieties/js/.js') }}"></script>
{% endblock %}